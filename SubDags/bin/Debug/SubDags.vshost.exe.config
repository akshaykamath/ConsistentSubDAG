<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--Change the file path here-->
    <add key="graphfilelocation" value="Graphs\mini-mfo2.txt" />
    <!--Make the graph mode value as 1 if the input is an adjacency matrix file -->
    <!--Make the graph mode value as 0 if the input is of child parent format -->
    <!-- Please put the child with root node as the first entry in the file after child parent!!!!-->
    <add key="graphmode" value="0" />
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />        
    </startup> 
    
  <runtime>
    <gcAllowVeryLargeObjects enabled="true" />
  </runtime>
</configuration>